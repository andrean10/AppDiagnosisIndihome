<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tolls="http://schemas.android.com/tools"
    android:orientation="horizontal">

    <TextView
        style="@style/style_table_header"
        android:id="@+id/tv_table_result_rank"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/table_content_cell"
        android:layout_gravity="top"
        tolls:text="No" />
    <TextView
        style="@style/style_table_header"
        android:id="@+id/tv_table_result_case"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top"
        android:background="@drawable/table_content_cell"
        tolls:text="Kasus"/>
    <TextView
        style="@style/style_table_header"
        android:id="@+id/tv_table_result_disease"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top"
        android:background="@drawable/table_content_cell"
        tolls:text="Penyakit"/>
    <TextView
        style="@style/style_table_header"
        android:id="@+id/tv_table_result_match"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top"
        android:background="@drawable/table_content_cell"
        tolls:text="Kecocokan"/>
</LinearLayout>