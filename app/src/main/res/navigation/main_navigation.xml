<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.kontrakanprojects.appbekamcbr.view.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_consultFragment2"
            app:destination="@id/consultFragment" />
        <action
            android:id="@+id/action_homeFragment_to_daftarPenyakitFragment"
            app:destination="@id/daftarPenyakitFragment" />
        <action
            android:id="@+id/action_homeFragment_to_daftarSolusiFragment"
            app:destination="@id/daftarSolusiFragment" />
        <action
            android:id="@+id/action_homeFragment_to_infoActivity"
            app:destination="@id/infoActivity" />
    </fragment>
    <fragment
        android:id="@+id/consultFragment"
        android:name="com.kontrakanprojects.appbekamcbr.view.consult.ConsultFragment"
        android:label="fragment_consult"
        tools:layout="@layout/fragment_consult" />
    <fragment
        android:id="@+id/daftarPenyakitFragment"
        android:name="com.kontrakanprojects.appbekamcbr.view.info.daftarpenyakit.DaftarPenyakitFragment"
        android:label="fragment_daftar_penyakit"
        tools:layout="@layout/fragment_daftar_penyakit" />
    <fragment
        android:id="@+id/daftarSolusiFragment"
        android:name="com.kontrakanprojects.appbekamcbr.view.info.daftarsolusi.DaftarSolusiFragment"
        android:label="fragment_daftar_solusi"
        tools:layout="@layout/fragment_daftar_solusi" />
    <activity
        android:id="@+id/infoActivity"
        android:name="com.kontrakanprojects.appbekamcbr.view.info.InfoActivity"
        android:label="activity_info"
        tools:layout="@layout/activity_info" />
</navigation>